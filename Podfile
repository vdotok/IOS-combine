    pod "VisualEffectView"
    pod 'MMWormhole'
    pod 'CocoaMQTT' , '2.0.3-beta2'
    

end

target 'ScreenShare' do
  # Pods for screen share
    use_frameworks!
    pod 'MMWormhole'
    pod 'VdoTokStreaming', :git => 'https://github.com/vdotok/VdoTokStreaming.git'
    
end
